    {
      "parameters": {
        "jsCode": "const markdown = $node[\"AI Agent3\"].json[\"output\"];\n\n// Simple markdown to HTML conversion\nlet html = markdown\n  .replace(/### (.*)/g, '<h3>$1</h3>')\n  .replace(/## (.*)/g, '<h2>$1</h2>')\n  .replace(/\\*\\*(.*?)\\*\\*/g, '<b>$1</b>')\n  .replace(/\\* (.*)/g, '<li>$1</li>')\n  .replace(/\\n/g, '<br>');\n\n// Wrap in professional template\nhtml = `\n<html>\n<head>\n<style>\nbody {\n  font-family: Arial, sans-serif;\n  line-height: 1.7;\n  margin: 50px;\n}\nh2 {\n  font-size: 24px;\n  color: #222;\n}\nh3 {\n  font-size: 18px;\n  color: #444;\n}\nli {\n  margin-bottom: 8px;\n}\n</style>\n</head>\n<body>\n${html}\n</body>\n</html>\n`;\n\nreturn [{ json: { html } }];"
      },
